{
  "name": "kr-core-gomi-keyword-front-user",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "yarn set version berry && yarn install",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "build:dev": "tsc && vite build --mode development",
    "build:prod": "tsc && vite build",
    "serve:dev": "yarn build:dev && vite preview",
    "serve:prod": "yarn build:prod && vite preview",
    "gen": "graphql-codegen --config codegen.yml && sed -i '' 's/= unknown/extends unknown/g' src/generated/graphql.tsx && sed -i '' 's/TData =/TData extends/g' src/generated/graphql.tsx && prettier --write src/generated/graphql.tsx"
  },
  "dependencies": {
    "@tanstack/react-query": "^4.13.0",
    "axios": "^0.27.2",
    "casing": "^1.1.0",
    "graphql": "^16.6.0",
    "graphql-request": "^4.3.0",
    "graphql-tag": "^2.12.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.39.6",
    "react-router-dom": "^6.4.2",
    "react-toastify": "^9.1.1",
    "react-tooltip": "^5.7.3",
    "recoil": "^0.7.6"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.13.8",
    "@graphql-codegen/client-preset": "^1.1.3",
    "@graphql-codegen/introspection": "^2.2.1",
    "@graphql-codegen/typescript": "^2.8.1",
    "@graphql-codegen/typescript-operations": "^2.5.6",
    "@graphql-codegen/typescript-react-query": "^4.0.5",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tanstack/react-query-devtools": "^4.14.5",
    "@types/node": "^18.11.7",
    "@types/react": "^18.0.22",
    "@types/react-dom": "^18.0.7",
    "@vitejs/plugin-react": "^2.2.0",
    "autoprefixer": "^10.4.13",
    "daisyui": "^2.38.1",
    "graphql-config": "^4.3.6",
    "postcss": "^8.4.18",
    "prettier": "latest",
    "react-svg": "^16.0.0",
    "tailwindcss": "^3.2.1",
    "vite": "3.1.8",
    "vite-plugin-svgr": "^2.2.2"
  },
  "packageManager": "yarn@3.3.1"
}
