{
  "name": "kr-core-gomi-keyword-front",
  "private": true,
  "workspaces": {
    "packages": [
      "apps/*"
    ]
  },
  "scripts": {
    "lint": "eslint ./apps --ext .ts,.tsx,.js,.jsx --cache",
    "prepare": "yarn set version berry && yarn install && yarn dlx @yarnpkg/sdks vscode",
    "user:dev": "yarn workspace kr-core-gomi-keyword-front-user run dev",
    "user:dev-build": "yarn workspace kr-core-gomi-keyword-front-user run build:dev",
    "user:prod-build": "yarn workspace kr-core-gomi-keyword-front-user run build:prod",
    "office:dev": "yarn workspace kr-core-gomi-keyword-front-office run dev",
    "office:dev-build": "yarn workspace kr-core-gomi-keyword-front-office run build:dev",
    "office:prod-build": "yarn workspace kr-core-gomi-keyword-front-office run build:prod",
    "office:prod-preview": "yarn workspace kr-core-gomi-keyword-front-office run serve:prod"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^8.26.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "prettier": "^2.8.0",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "ts-node": "^10.9.1",
    "tslib": "^1",
    "typescript": "^4.8.4"
  },
  "dependenciesMeta": {
    "eslint@8.26.0": {
      "unplugged": true
    }
  },
  "description": "gomi-insight",
  "repository": "https://github.com/gomicorp/kr-core-gomi-keyword-front.git",
  "packageManager": "yarn@3.3.1"
}
